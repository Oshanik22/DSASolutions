class Solution {
    public int lengthOfLastWord(String s) {
        int length=0;
        s = s.trim();
        
        for(int i=0; i<s.length(); i++){
            if(s.charAt(i)!=' '){
                length++;
            }
            else if(s.charAt(i)==' '){
                length=0;
            }
        }
        return length;
    }
}

